package jp.learningdesign.javastudy;

public class DisplayMonth {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		int month = 8;
		switch(month) {
		case 1:
			System.out.println("January");
			break;
		case 1:
			System.out.println("January");
			break;
		case 1:
			System.out.println("January");
			break;
		case 1:
			System.out.println("January");
			break;		case 1:
				System.out.println("January");
				break;		case 1:
					System.out.println("January");
					break;		case 1:
						System.out.println("January");
						break;

		}
	}

}
